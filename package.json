{
  "name": "@builderhub/eslint-config",
  "version": "0.0.8",
  "description": "Builderhub Platform Dev team ESLint configuration",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "release": "standard-version --no-verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/builderhub-platform/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "builderhub"
  ],
  "author": {
    "name": "Eunchurn Park",
    "email": "eunchurn@builderhub.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/builderhub-platform/eslint-config/issues"
  },
  "homepage": "https://github.com/builderhub-platform/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.1.0"
  },
  "devDependencies": {
    "@types/eslint": "8.44.7",
    "@types/node": "20.9.1",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "=>5.48.1",
    "@typescript-eslint/parser": "=>5.48.1",
    "eslint": "=>8.33.0",
    "eslint-config-prettier": "=>8.6.0",
    "eslint-plugin-prettier": "=>4.2.1",
    "prettier": ">=2.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
